{
  "dependencies": {
    "axios": "^0.19.2",
    "express": "4.17.1",
    "express-session": "^1.17.1",
    "google-auth-library": "^6.0.0",
    "morgan": "^1.10.0",
    "pg": "^8.0.3",
    "sequelize": "^5.21.7"
  },
  "scripts": {
    "debug": "NODE_ENV=development nodemon --inspect=0.0.0.0:9229 run.js",
    "start": "node run.js",
    "test": "NODE_ENV=test jest",
    "test:unit": "NODE_ENV=test jest -c jest.config.unit.js",
    "test:integration": "NODE_ENV=test jest -c jest.config.integration.js",
    "db:drop": "npx sequelize-cli db:drop",
    "db:create": "npx sequelize-cli db:create",
    "db:test:create": "NODE_ENV=test npx sequelize-cli db:create",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:test:migrate": "NODE_ENV=test npx sequelize-cli db:migrate",
    "db:migrate:undo": "npx sequelize-cli db:migrate:undo",
    "db:seed": "npx sequelize-cli db:seed:all",
    "db:seed:undo": "npx sequelize-cli db:seed:undo:all",
    "db:reset": "npm run db:drop && npm run db:create && npm run db:migrate && npm run db:seed"
  },
  "devDependencies": {
    "dotenv": "^8.2.0",
    "jest": "^26.0.1",
    "js-yaml": "^3.13.1",
    "nodemon": "^2.0.3",
    "sequelize-cli": "^5.5.1"
  }
}
